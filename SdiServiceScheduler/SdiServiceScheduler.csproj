        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>SDI Service Scehduler</Title>
        <Description>SDI Service Scehduler</Description>
        <PackageIcon>SDIIcon.jpg</PackageIcon>
